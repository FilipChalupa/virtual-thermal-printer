{
	"version": "0.0.0",
	"fmt": {
		"useTabs": true,
		"semiColons": false,
		"singleQuote": true
	},
	"imports": {
		"hono": "jsr:@hono/hono@^4.6.5",
		"hono/cors": "jsr:@hono/hono@^4.6.5/cors",
		"hono/deno": "jsr:@hono/hono@^4.6.5/deno",
		"@std/cli/parse-args": "jsr:@std/cli@^1.0.0/parse-args",
		"@std/assert/mod.ts": "https://deno.land/std@0.218.2/assert/mod.ts",
		"iconv-lite": "https://esm.sh/iconv-lite@0.6.3",
		"@std/io/buffer": "https://deno.land/std@0.224.0/io/buffer.ts",
		"imagescript": "https://deno.land/x/imagescript@v1.2.14/mod.ts",
		"@std/encoding/base64": "jsr:@std/encoding@^0.224.0/base64"
	},
	"tasks": {
		"start": "deno run --allow-net --allow-read main.ts",
		"dev": "deno run --allow-net --allow-read --watch main.ts",
		"escpos_test": "deno test --allow-net escpos.test.ts",
		"test": "deno test",
		"check": "deno check main.ts escpos.ts escpos.test.ts escpos-transform.ts",
		"fmt": "deno fmt",
		"lint": "deno lint"
	}
}
